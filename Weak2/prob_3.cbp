#include <stdio.h>
#include <stdlib.h>

int main()
{
    int N ,initail_val=1 ;
    int end_val = initail_val+3;
    int sum=1;
    printf("Enter Numbers of lines : ");
    scanf("%i",&N);
    for(int i=0;i<N;i++)
    {
        for(initail_val=sum;initail_val<end_val;initail_val++)
        {
            printf("%i ",initail_val);
        }
        printf("PMU");
        sum = initail_val+1;
        end_val = initail_val+4;
        printf("\n");
    }
    return 0;
}
