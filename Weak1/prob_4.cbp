#include <stdio.h>
#include <stdlib.h>

int main()
{
    float num1 ,num2;
    printf("Enter two numbers : ");
    scanf("%f %f",&num1,&num2);
    printf("Result of addition is : %f \n",num1+num2);
    printf("Result of subtraction is : %f \n",num1-num2);
    printf("Result of multiplication is : %f \n",num1*num2);
    printf("Result of division is : %f \n",num1/num2);
    return 0;
}
