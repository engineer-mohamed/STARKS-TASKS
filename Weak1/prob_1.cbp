#include <stdio.h>
#include <stdlib.h>


int main()
{
    int value_int;
    char value_char;
    float value_float;
    printf("Enter the value of int,char and float : ");
    scanf(" %i ",&value_int);
    scanf("%c",&value_char);
    scanf("%f",&value_float);
    printf("the int value : %i \n",value_int);
    printf("the char value : %c \n",value_char);
    printf("the float value : %f \n",value_float);
    return 0;
}

